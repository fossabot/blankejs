{
  "name": "BlankE",
  "description": "BlankE game dev",
  "version": "0.1.0",
  "main": "src/index.html",
  "single-instance": false,
  "window": {
    "frame": false,
    "transparent": true,
    "width": 800,
    "height": 600,
    "icon": "src/logo.png"
  },
  "build": {
    "nwVersion": "0.36.2",
    "nwFlavor": "normal",
    "files": [
      "./src/**/**"
    ],
    "mac": {
      "icon": "./src/logo.icns"
    },
    "win": {
      "icon": "./src/logo.ico"
    }
  },
  "scripts": {
    "less": " ",
    "nw": "nw .",
    "dist": "build --tasks win-x86 --mirror https://dl.nwjs.io/ .",
    "dist-linux": "build --tasks linux-x64 --mirror https://dl.nwjs.io/ .",
    "dist-mac": "build --tasks mac-x64 --mirror https://dl.nwjs.io/ .; cp -R love2d dist/BlankE-0.1.0-mac-x64/BlankE.app/Contents/Resources/app.nw/love2d",
    "start": "nw-react-scripts start",
    "build": "nw-react-scripts build",
    "test": "nw-react-scripts test",
    "eject": "nw-react-scripts eject"
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "archiver": "^2.1.1",
    "commonmark": "^0.28.1",
    "fs-extra": "^5.0.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "node-watch": "^0.6.0",
    "nw-react-scripts": "^2.0.0",
    "react": "^16.8.6",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "request": "^2.88.0",
    "walk": "^2.3.13"
  },
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.14.8"
  },
  "eslintConfig": {
    "extends": "nw-react-app"
  },
  "eslintIgnore": [
    "src/includes/**"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nwBuilder": {
    "//": "https://github.com/nwjs-community/nw-builder",
    "platforms": [
      "osx64",
      "win64"
    ],
    "version": "latest",
    "flavor": "normal",
    "buildDir": "./build",
    "cacheDir": "./cache",
    "macIcns": "./src/logo.icns"
  }
}
